{
   "name": "onboard-hugo-miranda",
   "version": "1.0.0",
   "main": "src/server.ts",
   "repository": "https://github.com/indigotech/onboard-hugo-miranda.git",
   "author": "hugo miranda <hugo.miranda.souza@gmail.com>",
   "license": "MIT",
   "scripts": {
      "build": "rm -rf build/ && yarn tsc",
      "start": "ts-node -r tsconfig-paths/register src/server.ts",
      "test": "ENV=TEST mocha -r ts-node/register -r tsconfig-paths/register --colors src/test/index.spec.ts --exit",
      "dev": "ENV=DEV nodemon -r tsconfig-paths/register -w src --ext ts --exec ts-node src/server.ts",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js",
      "seeds": "ts-node -r tsconfig-paths/register src/db/run.seeds.ts",
      "test:seeds": "ENV=TEST ts-node -r tsconfig-paths/register src/db/run.seeds.ts",
      "eslint:verify": "yarn eslint ./src/**"
   },
   "dependencies": {
      "apollo-server": "2.19.1",
      "bcryptjs": "2.4.3",
      "dotenv": "8.2.0",
      "graphql": "15.4.0",
      "jsonwebtoken": "8.5.1",
      "parse-duration": "0.4.4",
      "pg": "8.5.1",
      "reflect-metadata": "0.1.10",
      "typeorm": "0.2.29"
   },
   "devDependencies": {
      "@types/bcryptjs": "2.4.2",
      "@types/chai": "4.2.14",
      "@types/faker": "5.1.5",
      "@types/jsonwebtoken": "8.5.0",
      "@types/mocha": "8.2.0",
      "@types/supertest": "2.0.10",
      "@typescript-eslint/eslint-plugin": "4.12.0",
      "@typescript-eslint/parser": "4.12.0",
      "chai": "4.2.0",
      "eslint": "7.17.0",
      "eslint-config-prettier": "7.1.0",
      "eslint-plugin-prettier": "3.3.1",
      "faker": "5.2.0",
      "mocha": "8.2.1",
      "nodemon": "2.0.7",
      "prettier": "2.2.1",
      "prettier-eslint": "12.0.0",
      "supertest": "6.0.1",
      "ts-node": "3.3.0",
      "tsconfig-paths": "3.9.0",
      "typescript": "4.1.3"
   }
}
